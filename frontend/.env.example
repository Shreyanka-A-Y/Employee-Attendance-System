# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API URL
# For local development: http://localhost:5000/api
# For production: https://your-backend-service.onrender.com/api
# IMPORTANT: Make sure the URL ends with /api
REACT_APP_API_URL=http://localhost:5000/api

# Note: NODE_ENV is automatically set by React Scripts
# - 'development' when running npm start
# - 'production' when running npm run build
